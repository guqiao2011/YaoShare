<% if(page.type==="about"){ %>
<%- partial('about') %>
<% }else if(page.type==="child"){ %>
<%- partial('index') %>
<% }else{ %>	 
    <article class="page text-center">
    <div id="post-meta-m">
        <h3><%= page.title %></h3>
    </div>
    <div class="article-m">
    <div class="post-toc text-left">
        <% if (page.toc == true){ %>
            <%- toc(page.content) %>
        <% } %>
    </div>
    <div id="article" class="markdown-body textretty text-left" style="padding-left:32px;padding-right=32px;font-size:20px;">
        <%- page.content %>
    </div>
    </div>
    </article>
<% } %>
